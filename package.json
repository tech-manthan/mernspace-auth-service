{
  "name": "service-template",
  "version": "1.0.0",
  "description": "Backend Service Template",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --watch --runInBand",
    "format:check": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prepare": "husky",
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tech-manthan/backend-service-template.git"
  },
  "author": "Manthan Sharma <techmanthansharma@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tech-manthan/backend-service-template/issues"
  },
  "homepage": "https://github.com/tech-manthan/backend-service-template#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@types/supertest": "^6.0.3",
    "bcrypt": "^6.0.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.4",
    "typeorm": "0.3.25",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
